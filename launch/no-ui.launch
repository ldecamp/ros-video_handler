<launch>
    <node pkg="video_handler" type="vhandler" name="vhandler" clear_params="true" output="screen">
        <!-- 0 camera device | 1 filepath | 2 database -->
        <!-- <param name="source" value="2" /> -->
        <param name="source" value="1" />
        <!-- for camera id of the device (ex:0)  -->
        <!-- for filepath mode absolute path to video file (ex: /mnt/hgfs/Data/videos/ants/18/A18R2.m2ts) -->
        <!-- for database absolute filepath to database  (ex: /mnt/hgfs/Data/analysis/input.db)-->
        <!-- <param name="input" value="/mnt/hgfs/Data/videos/ants/1/Ant1ZVOP.m2ts" /> -->
        <param name="input" value="/mnt/hgfs/Data/videos/ants/2/Ant2ZVSF.m2ts" />
        <!-- <param name="input" value="/mnt/hgfs/Data/analysis/video_paths/ant2.db" /> -->
        <param name="sendReset" value="true" />
        <!-- Name of the topic to publish to -->
        <param name="topicname" value="/camera/image_raw" />
        <!-- Camera frame rate -->
        <param name="framerate" value="30" />

        <!-- Image transformation to rezise the frame -->
        <param name="resize" value="true" />
        <param name="newWidth" value="960" />
        <param name="newHeigth" value="540" />

        <!-- <param name="play_reversed" value="false" /> -->
    </node>
</launch>
