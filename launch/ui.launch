<launch>
    <node pkg="video_handler" type="vhandler" name="vhandler" clear_params="true" output="screen">
        <param name="stream_from_file" value="true" />
        <param name="device_id" value="0" />
        <param name="filepath" value="/mnt/hgfs/Data/videos/ants/2/Ant2R10.m2ts" />
        <param name="topicname" value="/camera/image_raw" />
        <param name="framerate" value="50" />

        <param name="resize" value="false" />
        <param name="newWidth" value="960" />
        <param name="newHeigth" value="540" />
    </node>
    <node name="image_view" pkg="image_view" type="image_view" respawn="false" output="screen">
    	<remap from="image" to="/camera/image_raw"/>
    	<param name="autosize" value="true" />
  	</node>
</launch>
